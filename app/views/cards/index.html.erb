<div id="container" ng-app='demos_cfg' ng-controller='LiveSearchController' class="section">
  <nav>
    <div class="nav-wrapper">
      <%= form_for(:query, method: :get, html: {class: "pure-form"}) do |f| %>
        <div class="input-field">
          <%= f.search_field :name, "ng-change" =>"change(text)", "ng-model" => "search" %>
          <label for="search"><i class="mdi-action-search"></i></label>
          <i class="mdi-navigation-close"></i>
        </div>
      <% end %>
    </div>
  </nav>

  <div class="card top-decks-box" ng-repeat="entry in entries">
    <div class="card-content blue-grey lighten-5">
      <span>
        {{entry.portuguese_name}}
      </span>
      <span class="secondary-content">
        {{entry.price}}
      </span>
    </div>
    <div class="card-action">
      <a href="#">action</a>
    </div>
  </div>
</div>
